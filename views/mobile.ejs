    <style>
        .title-header {
            animation: ChangeColor 5s linear infinite;
            font-size: 10px;
        }

        @keyframes ChangeColor {
            0% {
                color: red;
            }

            25% {
                color: orange;
            }

            50% {
                color: green;
            }

            75% {
                color: blue;
            }

            100% {
                color: violet;
            }
        }

        .image-overlay {
            position: relative;
            display: inline-block;
        }
        .overlay-content {
            position: absolute;
            top: 20%;
            left: 50%;
            width: 50%;
            height: 70%;
            transform: translate(-50%, -10%);
            color: white;
            background-color: rgba(0, 0, 0, 0.6);
            padding: 10px;
            text-align: center;
            width: 80%;
            border-radius: 10px;
            box-shadow: 0px 0px 10px 0px #888888;
        }

        .logo {
            width: 128px;
            height: 128px;
            position: absolute;
            top: 20%;
            left: 50%;
            border-radius: 100%;
            box-shadow: 0px 0px 10px 2px rgba(255, 255, 255, 0.5), 5px 5px 5px 2px rgba(0, 0, 0, 0.3);
            transform: translate(-50%, -50%);
        }

        .logo img {
            border-radius: 100%;

        }

        .title-name {
            position: absolute;
            top: 33%;
            left: 38%;
        }

        .boder-item {
            position: absolute;
            left: 15%;
            top: 42%;
            width: 70%;
            height: 50%;
            border-radius: 5px;
            border-width: 1px;
            box-shadow: 0px 0px 10px 2px rgba(255, 255, 255, 0.5), 5px 5px 5px 2px rgba(0, 0, 0, 0.3);
        }

        .boder-icon {
            width: 80px;
            height: 80px;
            margin-left: 38px;
            background-color: rgb(255, 255, 255);
            border-radius: 100px;
            display: flex;
            justify-content: center;
            align-items: center;
        }

        p {
            margin-top: 8px;
            font-size: 25px;
            font-family: 'Times New Roman', Times, serif;
        }

        .title-ins {
            margin-left: 24px;
        }

        .title-tiktok {
            margin-left: 35px;
        }

        a {
            color: inherit;
            text-decoration: none;
            margin: 0px;
            padding: 0px;
            background-color: transparent;
        }

        a:hover {
            color: inherit;
            text-decoration: none;
        }

        #grad {
            background-image: linear-gradient(to bottom right, #405DE6, #5851DB, #833AB4, #C13584, #E1306C, #FD1D1D, #F56040, #F77737, #FCAF45, #FFDC80);
            border-radius: 20px;
        }

        .heart {
            transform: rotate(0deg);
            animation: heart 0.6s linear infinite;
        }

        @keyframes heart {
            0% {
                transform: rotate(0deg) scale(1.00);
            }

            80% {
                transform: rotate(0deg) scale(0.90);
            }

            100% {
                transform: rotate(0deg) scale(0.80);
            }
        }
